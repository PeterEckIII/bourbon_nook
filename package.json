{
  "name": "bourbon-nook",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "db:connect:test": "npm run script \"PGPASSWORD=prisma psql -h localhost -p 5433 -d tests -U prisma\" test",
    "db:connect:dev": "",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npm run script \"prisma db seed\" confirm",
    "db:studio": "npm run script \"prisma studio\"",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down db",
    "dev": "remix vite:dev",
    "execute": "node --no-warnings --experimental-specifier-resolution=node --loader ./loader.js",
    "icons": "npm run script scripts/icons.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "script": "npm run execute scripts/setup.ts",
    "prod": "npm run script scripts/icons.ts prod confirm",
    "playwright:prepare": "./scripts/playwright.sh",
    "start": "remix-serve ./build/server/index.js",
    "preunit": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "unit": "npm run script \"vitest run --config ./vitest.config.unit.ts\" test",
    "postunit": "npm run db:down",
    "e2e:coverage": "nyc --reporter=lcov playwright test",
    "pree2e": "npm run script \"npm run db:up && npx prisma migrate deploy && npm run playwright:prepare\" test",
    "e2e": "playwright test",
    "poste2e": "npm run db:down",
    "pree2e:ui": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "e2e:ui": "playwright test --ui",
    "poste2e:ui": "npm run db:down",
    "preintegration": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "integration": "npm run script \"vitest run --config ./vitest.config.integration.ts\" test",
    "postintegration": "npm run db:down",
    "preintegration:ui": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "integration:ui": "npm run script \"vitest run --config ./vitest.config.integration.ts --ui\" test",
    "postintegration:ui": "npm run db:down",
    "typecheck": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.6",
    "@conform-to/validitystate": "^0.2.0",
    "@conform-to/zod": "^1.0.6",
    "@prisma/client": "^5.12.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^2.1.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "isbot": "^4.1.0",
    "lucide-react": "^0.365.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.5",
    "remix-themes": "^1.3.1",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.0",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@storybook/addon-actions": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.12.4",
    "@types/prompt": "^1.1.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/ui": "^1.4.0",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "happy-dom": "^14.4.0",
    "msw": "^2.2.13",
    "node-html-parser": "^6.1.13",
    "nyc": "^15.1.0",
    "playwright-test-coverage": "^1.2.12",
    "postcss": "^8.4.38",
    "prisma": "^5.12.0",
    "prompt": "^1.3.0",
    "remix-development-tools": "^4.1.4",
    "storybook": "^8.0.5",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.1.0",
    "vite-plugin-istanbul": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.4.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
